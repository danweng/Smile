<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - My ASP.NET Application</title>
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")
	<style>
		#content {
			background: url(vote.png) no-repeat;
			width: 677px;
			height: 724px;
		}

		input, button {
			position: absolute;
		}

		#gen1 {
			left: 255px;
			top: 595px;
		}

		#gen2 {
			left: 255px;
			top: 680px;
		}

		#fel1 {
			left: 440px;
			top: 569px;
		}

		#fel2 {
			left: 440px;
			top: 616px;
		}

		#fel3 {
			left: 440px;
			top: 662px;
		}

		#fel4 {
			left: 440px;
			top: 711px;
		}

		#submit {
			left: 650px;
			top: 505px;
			width: 135px;
			height: 235px;
			border: none;
			background: transparent;
			color: transparent;
			text-indent: -9999px;
		}
	</style>
</head>
<body>
	<div class="container body-content">
		@RenderBody()
	</div>

	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@RenderSection("scripts", required: false)
</body>
</html>
